{
  "name": "capstone-chat-server",
  "version": "0.0.0",
  "description": "캡스톤 채팅 서버",
  "type": "module",
  "main": "main.js",
  "imports": {
    "#src/*": "./src",
    "#DB/*": "./src/DB/*"
  },
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:coverage": "npm test -- --coverage",
    "test:watch": "npm test -- --watch",
    "start": "node src/main.js",
    "dev-start": "set ENV_NAME=.env.development.local&&nodemon --watch src --exec npm start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ShangBinLee/capstone-chat-server.git"
  },
  "author": "LeeShangBin",
  "license": "",
  "bugs": {
    "url": "https://github.com/ShangBinLee/capstone-chat-server/issues"
  },
  "homepage": "https://github.com/ShangBinLee/capstone-chat-server#readme",
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "mysql2": "^3.3.1",
    "node-fetch": "^3.3.1",
    "socket.io": "^4.6.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22"
  }
}
